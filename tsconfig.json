{
	"compilerOptions": {
		"baseUrl": ".",
		"emitDecoratorMetadata": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"lib": ["ES2022"],
		"module": "ES2022",
		"moduleResolution": "Node",
		"noImplicitAny": false,
		"outDir": "dist",
		"rootDir": "./",
		"sourceMap": true,
		"strict": true,
		"strictNullChecks": false,
		"target": "ES2022"
	},
	"exclude": ["**/*.config.ts", ".sst", "spec"]
}
