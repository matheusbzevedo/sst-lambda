{
	"compilerOptions": {
		"baseUrl": ".",
		"rootDir": "./",
		"lib": ["ES2022"],
		"module": "ES2022",
		"moduleResolution": "Node",
		"sourceMap": true,
		"target": "ES2022",
		"strict": true,
		"strictNullChecks": false,
		"noImplicitAny": false,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"esModuleInterop": true
	}
}
